[OSEv3:children]
masters
nodes

[OSEv3:vars]
ansible_ssh_user=root
deployment_type=origin
openshift_master_identity_providers=[{'name': 'htpasswd_auth', 'login': 'true', 'challenge': 'true', 'kind': 'HTPasswdPasswordIdentityProvider', 'filename': '/etc/origin/master/htpasswd'}]
containerized=true
openshift_release=v1.3
openshift_image_tag=v1.3.0
openshift_public_hostname=console.<%= @config.cluster.name %>.<%= @config.cluster.domain %>
openshift_master_default_subdomain=apps.<%= @config.cluster.name %>.<%= @config.cluster.domain %>
openshift_hosted_metrics_deploy=false

[masters]
console.<%= @config.cluster.name %>.<%= @config.cluster.domain %> openshift_schedulable=true

[nodes]
console.<%= @config.cluster.name %>.<%= @config.cluster.domain %> openshift_node_labels="{'region': 'infra', 'zone': 'default'}" openshift_schedulable=true
